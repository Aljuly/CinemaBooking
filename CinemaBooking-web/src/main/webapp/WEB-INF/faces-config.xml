<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<component>
	</component>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>managedBeans.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>hallBean</managed-bean-name>
		<managed-bean-class>managedBeans.HallBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>moovieBean</managed-bean-name>
		<managed-bean-class>managedBeans.MoovieBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>spBean</managed-bean-name>
		<managed-bean-class>managedBeans.SpBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>public/login.xhtml</display-name>
		<from-view-id>/public/login.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/secure/hallsinfo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secure/hallsinfo.xhtml</display-name>
		<from-view-id>/secure/hallsinfo.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/secure/viewhall.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secure/hallsinfo.xhtml</display-name>
		<from-view-id>/secure/hallsinfo.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/secure/createhall.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>secure/hallsinfo.xhtml</display-name>
		<from-view-id>/secure/hallsinfo.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/secure/halledit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>